# program to print longest word in a string

string = str(input())
notaAlpha = []
new = string.replace(" ", "")
for char in new:
    if not char.isalpha():
        notaAlpha.append(char)
print(notaAlpha)

for item in notaAlpha:
    string = string.replace(item, "")
print(string)

